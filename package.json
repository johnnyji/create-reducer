{
  "name": "create-reducer-redux",
  "version": "1.0.5",
  "description": "A clean and functional way to create Redux reducers",
  "main": "./lib/index.js",
  "scripts": {
    "test": "mocha test/test.js",
    "test-debug": "node-debug _mocha test/test.js --no-timeouts && open http://127.0.0.1:8080/?port=5858",
    "transpile": "babel ./src --watch --presets es2015,stage-0 --plugins add-module-exports --out-dir ./lib",
    "release-patch": "npm version patch -m \"Upgrade to %s\" && npm run transpile",
    "release-minor": "npm version minor -m \"Upgrade to %s\" && npm run transpile",
    "release-major": "npm version major -m \"Upgrade to %s\" && npm run transpile"
  },
  "keywords": [
    "redux",
    "reducer",
    "immutablejs",
    "immutable",
    "functional",
    "store",
    "flux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnnyji/create-reducer-redux"
  },
  "homepage": {
    "url": "https://github.com/johnnyji/create-reducer-redux"
  },
  "author": "Johnny Ji",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "expect": "^1.20.1",
    "immutable": "^3.8.1",
    "mocha": "^2.5.3"
  }
}
